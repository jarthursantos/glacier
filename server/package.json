{
  "name": "glacier",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "cross-env DEBUG=glacier:* ts-node-dev -r tsconfig-paths/register --files --respawn src/server.ts",
    "dev:queue": "cross-env DEBUG=glacier:* ts-node-dev -r tsconfig-paths/register --files --respawn src/queue.ts",
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist --extensions \".ts,.tsx\" --copy-files --source-maps inline",
    "start": "node dist/server.js",
    "test": "jest"
  },
  "dependencies": {
    "@aws-sdk/client-glacier": "^3.8.0",
    "@aws-sdk/client-s3": "^3.8.1",
    "@aws-sdk/client-sns": "^3.8.0",
    "awilix": "^4.3.3",
    "bull": "^3.20.1",
    "date-fns": "^2.19.0",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-errors": "^1.8.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/plugin-transform-typescript": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bull": "^3.15.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/node": "^14.14.33",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.6",
